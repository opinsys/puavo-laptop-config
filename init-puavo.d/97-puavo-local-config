puavo_personally_administered_device=$(jq -r .personally_administered_device \
				             /etc/puavo/device.json)

if [ "$puavo_personally_administered_device" = "true" ]; then
  /usr/sbin/puavo-local-config --admins         \
                               --grub-default   \
                               --local-users    \
                               --setup-pkgs all
fi
